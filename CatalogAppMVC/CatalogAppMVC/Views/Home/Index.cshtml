@model List<CatalogAppMVC.Models.Category>
@{
    ViewBag.Title = "Index";
}

<ul class="nav nav-pills">
    <li class="active">
        @Html.ActionLink("Разделы", "Index", "Home")
    </li>
</ul>

<ul class="list-group">
    @foreach (CatalogAppMVC.Models.Category category in Model)
    {
        <li class="list-group-item">@Html.ActionLink(category.Name, "RecordsOfCategory", "Records", new { categoryID = category.ID }, null)</li>
    }
</ul>

